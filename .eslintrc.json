{
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"eslint-config-ultraq"
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "script"
	},
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"no-unused-vars": ["error", {
			"ignoreRestSiblings": true
		}],
		"react/prop-types": "off"
	}
}
